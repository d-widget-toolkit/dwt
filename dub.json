{
    "name":        "dwt",
    "description": "A library for creating cross-platform GUI applications.",
    "authors":     ["Jacob Carlborg", "Frank Benoit"],
    "license":     "EPL",
	"targetType": "library",
	"sourcePaths-windows": ["base/src", "org.eclipse.swt.win32.win32.x86/src"],
	"stringImportPaths-windows": ["org.eclipse.swt.win32.win32.x86/res"],
	"libs-windows": ["advapi32", "comctl32", "comdlg32", "gdi32", "kernel32", "shell32", 
						"ole32", "oleaut32", "olepro32", "oleacc", "user32", 
						"usp10", "msimg32", "opengl32", "shlwapi"],
	"lflags-windows": ["/NOM", "+$PACKAGE_DIR\\\\lib\\\\"],
	"dflags-windows": ["$PACKAGE_DIR\\win-res\\\\resource.res"],
	"preGenerateCommands-windows": ["if not exist \"$PACKAGE_DIR\\imp\" cd $PACKAGE_DIR && rdmd build base swt"],
	"buildRequirements": ["silenceWarnings"]
}